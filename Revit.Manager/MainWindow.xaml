<Window x:Class="ModBox.FamFactory.Revit.Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="698" Width="961.333" d:DataContext="{Binding famFactoryViewModel, Source={StaticResource FFVM}}" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame VerticalAlignment="Top" HorizontalAlignment="Center" Grid.ColumnSpan="2" NavigationUIVisibility="Hidden" Content="{Binding SelectedElement}" />
        <Button Grid.Row="1" />
        <Button Grid.Row="1" Grid.Column="1" Command="{Binding NextElementCommand, Mode=OneWay}" >
        </Button>
    </Grid>
</Window>
